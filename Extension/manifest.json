{
  "manifest_version": 2,

  "name": "DAGR Saver",
  "description": "Create a DAGR from any webpage.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_title": "DAGR Saver",
   "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["popup.js"]
  },
  "permissions": ["activeTab"]
}
